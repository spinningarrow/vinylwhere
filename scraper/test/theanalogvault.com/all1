  <div class="details ">

    <div class="module header">
      <span class="close-modal">v</span>
      <p class="brand">RCA</p>
      <p class="title"><a href="/products/alicia-keys-here">Alicia Keys ‎– Here</a></p>
      
        <p class="price"><span class="money">$39.00</span></p>
      
    </div>

    <form class="module options quick-shop-form unavailable" action="/cart/add" method="post" id="quick-shop-form-9646715916">

      

